          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import groovy.json.JsonSlurper
import org.apache.commons.io.FileUtils
import java.nio.charset.StandardCharsets

// Парсим JSON из ответа предыдущего запроса
def response = prev.getResponseDataAsString()
def jsonSlurper = new JsonSlurper()
def jsonResponse = jsonSlurper.parseText(response)

// Извлекаем массив `id` из объекта `data`
def ids = jsonResponse.data.collect { it.id }

// Определяем файл, в который будет производиться запись
def file = new File(&quot;ids.csv&quot;)

// Удаляем файл, если он уже существует
if (file.exists()) {
    file.delete()
}

// Создаем файл заново и записываем в него заголовок &quot;id&quot;
// Далее записываем каждый `id` в файл CSV, каждый `id` на новой строке
FileUtils.writeStringToFile(file, &quot;id\n&quot;, StandardCharsets.UTF_8) // Записываем заголовок
ids.each { id -&gt;
    FileUtils.writeStringToFile(file, &quot;${id}\n&quot;, StandardCharsets.UTF_8, true) // Дозаписываем id
}</stringProp>
